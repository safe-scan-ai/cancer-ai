      "justMyCode": false
    },
    {
      "name": "Testnet Validator UID 87",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/neurons/validator.py",
      "console": "integratedTerminal",
      "args": [
        "--wallet.name=testnet_vali_2",
        "--wallet.hotkey=vali2_hotkey",
        "--subtensor.network=test",
        "--logging.trace",
        "--netuid=163",
        "--db_path=./testnet_chain_format.db",
        "--wandb.off"
      ],
      "env": {
        "PYTHONPATH": "${workspaceFolder}:${PYTHONPATH}"
      },
      "justMyCode": false
