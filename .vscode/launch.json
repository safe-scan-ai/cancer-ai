{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Validator",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/neurons/validator.py",
      "console": "integratedTerminal",
      "args": [
        "--wallet.name=validator-staked",
        "--wallet.hotkey=default",
        "--netuid=163",
        "--logging.debug",
        "--subtensor.network", "test",
        "--datasets_config_hf_repo_id", "safescanai/competition-configuration-testnet",
